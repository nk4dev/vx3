model Info {
  id    String @id @default(cuid())
  name String
  metadata Metadata[]
  owner  Owner   @relation(fields: [userId], references: [id])
  userId String
}

model Owner {
    id    String @id @default(cuid())
    name  String
    email String @unique
    info  Info[]
}

model Metadata {
  id    String @id @default(cuid())
  info  Info   @relation(fields: [infoId], references: [id])
  infoId String
  icon  String
  description String
}