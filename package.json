{
	"name": "vx3",
	"private": true,
	"version": "0.0.18",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"dev": "turbo dev",
		"sdk:server": "turbo -F vx3-sdk dev",
		"sdk:build": "turbo -F vx3-sdk build",
		"sdk:start": "turbo -F vx3-sdk dev:serve",
		"system": "turbo run dev --filter=system",
		"build": "turbo build",
		"check-types": "turbo check-types",
		"dev:web": "turbo -F web dev",
		"dev:server": "turbo -F server dev",
		"db:push": "turbo -F server db:push",
		"db:studio": "turbo -F server db:studio",
		"db:generate": "turbo -F server db:generate",
		"db:migrate": "turbo -F server db:migrate"
	},
	"dependencies": {},
	"devDependencies": {
		"turbo": "^2.5.4"
	},
	"packageManager": "npm@10.9.2"
}
